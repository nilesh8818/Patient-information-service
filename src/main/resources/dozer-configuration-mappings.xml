<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<configuration>
		<copy-by-references>
			<copy-by-reference>java.time.*</copy-by-reference>
		</copy-by-references>
	</configuration>
	<mapping>
	<class-a>com.company.patinet.pojo.Patient</class-a>
	<class-b>com.company.patinet.exposedpojo.PatientExposed</class-b>
	
	
	<field>
	<a>patientId</a>
	<b>patientId</b>
	</field>
	
	<field>
	<a>firstName</a>
	<b>firstName</b>
	</field>
	
	<field>
	<a>lastName</a>
	<b>lastname</b>
	</field>
	
	<field>
	<a>state</a>
	<b>state</b>
	</field>
	
	<field>
	<a>city</a>
	<b>city</b>
	</field>
	
	<field>
	<a>guid</a>
	<b>guid</b>
	</field>
	
	<field>
	<a>createdDate</a>
	<b>createdDate</b>
	</field>
	
	<field>
	<a>updatedDate</a>
	<b>updatedDate</b>
	</field>
	
	</mapping>
</mappings>